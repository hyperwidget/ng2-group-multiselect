{
  "name": "ng2-group-multiselect",
  "version": "0.0.10",
  "author": "Hunter Jansen<hunter@hyperwidget.com>(hyperwidget.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rawkamatic/ng2-group-multiselect"
  },
  "description": "A multi-select component for angular 2 using grouping",
  "keywords": [
    "angular2",
    "typescript"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack --watch --progress",
    "clean": "rimraf dist/",
    "cover": "remap-istanbul -i coverage/chrome/coverage-final.json -o coverage/coverage.json",
    "postcover": "istanbul report lcov text-summary",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "lint": "npm run lint-ts && npm run lint-js && npm run lint-css",
    "lint-ts": "tslint './src/**/*.ts'",
    "lint-js": "eslint '**/*.js' --ignore-path .gitignore",
    "lint-css": "stylelint './src/**/*.css'",
    "start": "cross-env NODE_ENV=production node server/node-server.js",
    "pretest": "rimraf coverage/",
    "test": "karma start",
    "posttest": "npm run cover",
    "test:watch": "karma start --singleRun false",
    "typings": "rimraf typings/ && typings install"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "autoprefixer-core": "^6.0.1",
    "classlist-polyfill": "^1.0.2",
    "codelyzer": "0.0.19",
    "console-polyfill": "^0.2.2",
    "copy-webpack-plugin": "^3.0.1",
    "core-js": "^2.3.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "cssnano": "^3.5.2",
    "es7-reflect-metadata": "^1.6.0",
    "eslint": "^2.9.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "helmet": "^2.0.0",
    "html-webpack-plugin": "^2.7.2",
    "html5-history-api": "^4.2.7",
    "http-proxy": "^1.13.2",
    "inline-source-map": "^0.6.2",
    "istanbul": "^0.4.3",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-source-map-support": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.9.1",
    "raf": "^3.2.0",
    "ramda": "^0.21.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.3",
    "remap-istanbul": "^0.6.3",
    "rimraf": "^2.5.1",
    "rxjs": "^5.0.0-beta.6",
    "selenium-standalone": "^5.1.0",
    "style-loader": "^0.13.0",
    "stylelint": "^6.3.2",
    "stylelint-webpack-plugin": "^0.2.0",
    "to-string-loader": "^1.1.3",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.3.0",
    "tslint-loader": "^2.1.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.9.1",
    "webpack-split-by-path": "0.0.8",
    "winston": "^2.1.1",
    "zone.js": "0.6.12"
  }
}
